{
  "source": "./",
  "destination": "./docs",
  "excludes": ["(docs|node_modules|dist|src/Globals/extern)"],
  "index": "./readme.md",
  "package": "./package.json",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "dynamicImport": true,
        "lint": {"enable": true},
        "coverage": {"enable": true},
        "accessor": {
          "access": ["public", "protected", "private"],
          "autoPrivate": true
        },
        "undocumentIdentifier": {"enable": true},
        "unexportedIdentifier": {"enable": false},
        "typeInference": {"enable": true},
        "brand": {
          "title": "Kundeportal",
          "description": "Kundeportal f√ºr Advanced Unibyte",
          "repository": "http://svaurtdev:7990/scm/cmsdat/cmsdat_snap-watcher.git",
          "site": "#",
          "author": ["https://github.com/Megajin", "https://github.com/jokuebler"]
        }
      }
    },
    {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "classProperties": true,
        "objectRestSpread": true,
        "doExpressions": true,
        "functionBind": true,
        "functionSent": true,
        "asyncGenerators": true,
        "decorators": true,
        "exportExtensions": true,
        "dynamicImport": true
      }
    }
  ]
}
